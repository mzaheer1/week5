<p id="notice"><%= notice %></p>

<p>
  <b>Title:</b>
  <%= @event.title %>
</p>

<p>
  <b>Time:</b>
  <%= @event.time %>
</p>

<p>
  <b>Venue:</b>
  <ul>
    <% @event.venues.each do |venue| %>
      <li><%= link_to venue.name, venue %></li>
    <% end %>
  </ul>
</p>


<%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'Back', events_path %>
